<?xml version="1.0" encoding="utf-8"?>
    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:background="@android:color/white"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.miituo.atlaskm.activities.TokenSmsActivity">

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="30dp"
        android:layout_marginStart="8dp"
        android:layout_toEndOf="@+id/textView33"
        android:onClick="openView"
        android:text="Cotiza"
        android:theme="@style/textThemeEditSecondary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView33" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView29"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="30dp"
        android:layout_marginStart="30dp"
        android:text="¿No estas asegurado?"
        android:theme="@style/textThemePrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/editTextToken"
        app:layout_constraintTop_toTopOf="@+id/textView34" />

    <Button
        android:id="@+id/button"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView33"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="20dp"
        android:onClick="loadInfo"
        android:text="Aceptar"
        android:theme="@style/buttonThemePrimary"
        android:background="@drawable/button"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextToken" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView28"
        android:layout_below="@+id/textView28"
        android:lineSpacingExtra="8sp"
        android:text="Introduce el código de seguridad que recibirás enseguida..."
        android:layout_marginTop="35dp"
        android:theme="@style/textThemePrimary"
        android:gravity="start"
        app:layout_constraintTop_toBottomOf="@+id/textView28"
        app:layout_constraintLeft_toLeftOf="@+id/textView28" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="0.12"
        android:autoText="false"
        android:clickable="false"
        android:text="Casi"
        android:textAllCaps="false"
        android:theme="@style/textThemePrimary"
        android:textColor="@color/naranjaatlas"
        android:textSize="22sp"
        tools:textAlignment="viewStart"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="90dp"
        android:layout_marginStart="50dp" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView28"
        android:layout_alignBottom="@+id/textView28"
        android:layout_toEndOf="@+id/textView28"
        android:text="listos"
        android:theme="@style/textThemePrimary"
        android:textSize="22sp"
        android:layout_marginStart="4dp"
        app:layout_constraintLeft_toRightOf="@+id/textView28"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="90dp" />

    <EditText
        android:id="@+id/editTextToken"
        android:layout_width="239dp"
        android:layout_height="44dp"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:hint="Token"
        android:theme="@style/textThemeEdit"
        android:background="@drawable/backspinner"
        android:inputType="textPersonName"
        app:layout_constraintTop_toBottomOf="@+id/textView32"
        app:layout_constraintLeft_toLeftOf="@+id/textView32" />

    <TextView
        android:id="@+id/textView59"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="sendTokenA"
        android:text="Enviar de nuevo"
        android:theme="@style/textThemeEditSecondary"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@+id/button" />

</androidx.constraintlayout.widget.ConstraintLayout>
