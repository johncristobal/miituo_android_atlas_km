<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="5dp"
    android:paddingLeft="5dp"
    android:paddingRight="5dp"
    android:paddingBottom="5dp"
    android:id="@+id/btnGral"
    tools:context="com.miituo.atlaskm.activities.MainActivity">

    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/iconPush"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="7dp"
        android:layout_marginLeft="7dp"
        android:layout_marginStart="7dp"
        android:layout_toEndOf="@+id/StateImage"
        android:layout_toRightOf="@+id/StateImage"
        android:src="@drawable/foto"
        app:civ_border_color="#FF000000"
        app:civ_border_width="0dp" />

    <View
        android:id="@+id/vi"
        android:layout_width="1dp"
        android:layout_height="60dp"
        android:background="#9e9e9e"
        android:layout_marginTop="7dp"
        android:layout_marginLeft="7dp"
        android:layout_marginStart="7dp"
        android:layout_toEndOf="@+id/iconPush"
        android:layout_toRightOf="@+id/iconPush"/>

</RelativeLayout>
